<meta name="title" content="{{wallpaper.title}}">
<meta name="description" content="{{wallpaper.description}}">
<meta property="og:title" content="{{wallpaper.title}}" />
<meta property="og:description" content="{{wallpaper.description}}" />

<meta property="og:type" content="website" />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:title" content="{{wallpaper.title}}" />
<meta name="twitter:description" content="{{wallpaper.description}}" />

{% if wallpaper.filename is defined %}
    <meta property="og:image" content="https://gloztik.com/public/image/webp/1000/{{wallpaper.filename}}.webp" />
    <meta property="og:url" content="https://gloztik.com/wallpaper/{{wallpaper.filename}}" />
    <meta name="twitter:image" content="https://gloztik.com/public/image/webp/1000/{{wallpaper.filename}}.webp" />
    <link rel="canonical" href="https://gloztik.com/wallpaper/{{wallpaper.filename}}" />
{% else %}
    <meta property="og:image" content="{{wallpaper.imgUrl}}" />
    <meta property="og:url" content="{{wallpaper.url}}" />
    <meta name="twitter:image" content="{{wallpaper.imgUrl}}" />
    <link rel="canonical" href="{{wallpaper.url}}" />
{% endif %}